{
	"$schema": "vscode://schemas/color-theme",
	"name": "Nebula Purple and Teal",
	"include": "./dark_defaults.json",
	"tokenColors": [

		//-----------------------------------
		// Default Values
		//-----------------------------------
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#efd8fc"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#b863ff"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#dbb3fc"
			}
		},
		{
			"scope": [
